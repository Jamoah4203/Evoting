{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node@20.x"
    },
    {
      "src": "client/**",
      "use": "@vercel/static-build@latest"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1",
      "continue": true
    }
  ]
}